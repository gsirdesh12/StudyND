<h1>Login</h1>

<%= form_tag authenticate_path do %>
  <p>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </p>

  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>

  <p>
    <%= submit_tag :Login %>
  </p>
  <p>
    <%= link_to 'Create Account', new_user_path %>
  </p>
<% end %>
